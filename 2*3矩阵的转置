#include<stdio.h>
int main()
{
 int a[2][3],i,j,b[3][2];
 for(i=0;i<2;i++)
 {
  for(j=0;j<3;j++)
  {
   scanf("%d",&a[i][j]);
  }
 }
 for(j=0;j<2;j++)
 {
  for(i=0;i<3;i++)
   {
    b[i][j]=a[j][i];
   }
 }
 for(i=0;i<3;i++)
 {
  for(j=0;j<2;j++)
  {
   printf("%d ",b[i][j]);
  }
  printf("\n");
 }
}
