#include<stdio.h>
int main()
{
	int a[10][10], i, j, n;
	scanf("%d", &n);
	for (i = 0; i < n; i++)
	{
		a[i][0] = 1;
		for (j = 0; j <= i; j++)
		{
			a[i][j] = 1;
			if (i >= 1 && j >= 1&&i!=j)
			{
				a[i][j] = a[i - 1][j - 1] + a[i - 1][j];
			}
			printf("%d ", a[i][j]);
		}
		printf("\n");
	}
}
