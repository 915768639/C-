#include<stdio.h>
int main()
{
 int a[8],i,n,m,t,flag;
 scanf("%d",&n);
 for(m=0,t=n;t!=0;m++)
 {
  t=t/10;
 }
 for(i=0,t=n;i<m;i++)
 {
  a[i]=t%10;
  t=t/10;
 }
 if(m%2==0)
 {
  for(i=0,flag=1;i<(m/2)&&flag==1;i++)
  {
   if(a[i]==a[m-1-i]);
   else
   {
    flag=0;
   }
  }
  if(flag==0)
  {
   printf("这不是回文数\n");
  }
  else
  {
   printf("这是个回文数\n");
  }
 }
 else
 {
  for(i=0,flag=1;i<((m+1)/2)&&flag==1;i++)
  {
   if(a[i]==a[m-1-i]);
   else
   {
    flag=0;
   }
  }
  if(flag==0)
  {
   printf("这不是回文数\n");
  }
  else
  {
   printf("这是个回文数\n");
  }
 }
}
